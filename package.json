{
  "name": "lit-html-try",
  "version": "1.0.0",
  "repository": "https://github.com/newbornfrontender/lit-html-try.git",
  "author": "Infant Frontender <newbornfrontender@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write **/*.{html,js,json,md}",
    "clean": "rimraf public/**/*.js",
